{
	"name": "@begit/core",
	"version": "0.2.0",
	"description": "Fast tool for cloning git repositories, with no reliance on local `git`, or `tar` installs",
	"author": "Thomas Beer",
	"license": "MIT",
	"homepage": "https://github.com/Tommypop2/begit",
	"repository": {
		"type": "git",
		"url": "https://github.com/Tommypop2/begit"
	},
	"type": "module",
	"files": [
		"dist",
		"types"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./utils": {
			"types": "./types/utils.d.ts",
			"import": "./dist/utils.js",
			"default": "./dist/utils.js"
		}
	},
	"scripts": {
		"test": "vitest run",
		"build": "tsc && tsup"
	},
	"keywords": [
		"git",
		"template",
		"scaffolding"
	],
	"devDependencies": {
		"@types/node": "^22.14.1",
		"@types/tar": "^6.1.13",
		"tsup": "^8.4.0",
		"typescript": "^5.8.3",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"tar": "^7.4.3"
	},
	"publishConfig": {
		"access": "public"
	}
}
